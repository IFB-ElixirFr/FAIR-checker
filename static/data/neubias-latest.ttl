<http://biii.eu/node/1072> a <http://biii.eu/node/software> ;
    nb:hasAuthor "orcid.org/0000-0003-0229-5722" ;
    nb:hasFunction <http://edamontology.org/operation_Pixel_classification>,
        <http://edamontology.org/operation__R9Qf06vxvFj2UbAk7pW3IMG>,
        <http://edamontology.org/operation__RBNng13hEB0CJD5VzSKqrfB> ;
