    <http://www.orpha.net/consor/cgi-bin/OC_Exp.php?lng=en&Expert=324972> a ns1:MedicalCondition ;
        ns1:alternateName "Mouth and genital ulcers-inflamed cartilage syndrome" ;
        ns1:code [ a ns1:MedicalCode ;
